#include <bits/stdc++.h>
using namespace std;
 
int main() {
	char num[15];
	int x,T;
	scanf("%d",&T);
	while(T--){
	    scanf("%s",num);
	    x=strlen(num);
	    if(x == 5) printf("3\n");
	    else{
	        if((num[0]=='t' && num[1]=='w') || (num[0]=='t' && num[2]=='o') || (num[1]=='w' && num[2]=='o'))
	            printf("2\n");
	        else printf("1\n");
	    }
	}
 
	return 0;
